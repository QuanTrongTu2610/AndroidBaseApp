        <attr name="color" format="color" />
        <attr name="labelText" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BarChartCustom">
        <attr name="defaultLabelColor" format="color" />
        <attr name="defaultColumnColor" format="color" />
        <attr name="defaultMarkerColor" format="color" />
        <attr name="defaultMarkerTextColor" format="color" />
        <attr name="marginBetweenColumn" format="dimension" />
        <attr name="selectedColumnColor" format="color" />
        <attr name="textMarkerSize" format="dimension" />
        <attr name="textLabelSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TabBarCustom">
        <attr name="underLineBackGroundColor" format="color" />
        <attr name="underLineForeGroundColor" format="color" />
        <attr name="textTabSize" format="dimension" />
